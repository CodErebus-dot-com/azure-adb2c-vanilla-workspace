{
  "name": "azure_react_ssr_b2c",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npm-run-all build:dev server",
    "clean": "rimraf dist",
    "build:dev": "npm-run-all clean -p webpackDev:*",
    "build:prod": "npm-run-all clean -p webpackProd:*",
    "devWatch": "npm-run-all -p buildWatch:dev server",
    "server": "npm-run-all node-env port && nodemon --watch dist --exec node dist/server.js",
    "startDevWatch:server": "yarn run webpackDev:server --watch",
    "startDevWatch:client": "yarn run webpackDev:client --watch --no-cache",
    "webpackProd:server": "webpack --config webpack/prod/webpack.prod.server.js",
    "webpackProd:client": "webpack --config webpack/prod/webpack.prod.client.js",
    "webpackDev:server": "webpack --config webpack/dev/webpack.dev.server.js",
    "webpackDev:client": "webpack --config webpack/dev/webpack.dev.client.js",
    "buildWatch:dev": "npm-run-all -p startDevWatch:*",
    "port": "cross-env PORT=4003",
    "node-env": "cross-env NODE_ENV=development",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "test:watch": "yarn test -- --watch",
    "test": "jest -c ./config/jest/jest.config.js",
    "analyze": "cross-env ANALYZE=true && yarn build",
    "env-var": "cross-env BASE_PATH=false"
  },
  "browserslist": [
    "> 0.25%",
    "not ie <= 11",
    "not edge < 15",
    "not op_mini all",
    "not dead"
  ],
